@model Connecto.BusinessObjects.Vendor
@{
    ViewBag.Title = "Vendor";
}
<div ng-app="cSettingApp">
    <div ng-view></div>
</div>
<script src="~/Scripts/jquery.dataTables.min.js"></script>
<script src="~/Scripts/jquery.dataTables.bootstrap.js"></script>
<script src="~/Scripts/connecto/csettings/vendorController.js"></script>
<script type="text/javascript">
    $('#menu-csettings-main').syncMenu([{ name: "vendors", option: "a" }]);
    cSettingApp.config(['$routeProvider',
    function ($routeProvider) {
        $routeProvider.
             when('/', { templateUrl: '/Vendor/List', controller: 'VendorListCtrl' })
            .when('/Create/', { templateUrl: '/Vendor/Create', controller: 'VendorNewCtrl' })
            .when('/Edit/:itemId', { templateUrl: '/Vendor/Edit', controller: 'VendorEditCtrl' })
            .otherwise({ redirectTo: '/' });
    }]);
</script>